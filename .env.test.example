# BeatMatchMe Test Environment Configuration
# Copy this file to .env.test and fill in your actual values

AWS_REGION=us-east-1
TEST_EVENTS_TABLE=beatmatchme-events-dev
TEST_REQUESTS_TABLE=beatmatchme-requests-dev
TEST_TRANSACTIONS_TABLE=beatmatchme-transactions-dev

# Override environment variables for Lambda functions
EVENTS_TABLE=beatmatchme-events-dev
REQUESTS_TABLE=beatmatchme-requests-dev
